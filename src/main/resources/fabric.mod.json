{
  "schemaVersion": 1,
  "id": "aurus",
  "version": "${version}",
  "name": "Aurus: Novae Terrae",
  "description": "A not so Vanilla-Friendly Adventure Mod!",
  "authors": ["Dupernite", "ImNotDai"],
  "contact": {
    "repo": "https://github.com/arthu/AurusMod"
  },
  "license": "MIT License",
  "icon": "assets/aurus/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.dupernite.aurus.AurusMod"
    ],
    "client": [
      "com.dupernite.aurus.AurusModClient"
    ],
    "fabric-datagen": [
      "com.dupernite.aurus.AurusModDataGenerator"
    ],
    "rei_client": [
      "com.dupernite.aurus.compat.AurusModREIClientPlugin"
    ]
  },
  "mixins": [
    "Aurus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}